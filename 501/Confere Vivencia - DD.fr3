    <TfrxDBXQuery Name="Sqlconfere" UserName="Sqlconfere" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT &#13;&#10;    CELA.CELA,&#13;&#10;   (SELECT INTERNO.NOME_INTERNO||' - ROUPA:'||COALESCE(INTERNO.NUMERO_ROUPA,'')&#13;&#10;    FROM INTERNO                         &#13;&#10;    WHERE &#13;&#10;    INTERNO.ST='A' AND&#13;&#10;    COALESCE(INTERNO.EM_TRANSITO,'N')='N' AND        &#13;&#10;    INTERNO.idcela = CELA.id_cela) NOME_INTERNO,&#13;&#10;    NULL DATA_ENTRADA,&#13;&#10;    PAVILHAO.PAVILHAO,&#13;&#10;    GALERIA.GALERIA,&#13;&#10;    CELA.CELA,&#13;&#10;    SOLARIO.SOLARIO,&#13;&#10;    case solario.dia_visita&#13;&#10;    when '1' then 'DOMINGO'&#13;&#10;    when '2' then 'SEGUNDA'&#13;&#10;    when '3' then 'TERÇA'&#13;&#10;    when '4' then 'QUARTA'&#13;&#10;    when '5' then 'QUINTA'&#13;&#10;    when '6' then 'SEXTA'&#13;&#10;    when '7' then 'SABADO'&#13;&#10;    when '8' then 'NÃO TEM'&#13;&#10;    end as dia_visita,&#13;&#10;    case solario.periodo_visita&#13;&#10;    when 'M' then 'MANHÃ'&#13;&#10;    when 'T' then 'TARDE'&#13;&#10;    end as periodo_visita,&#13;&#10;    COALESCE((SELECT COUNT(*)              &#13;&#10;              FROM INTERNO                         &#13;&#10;              WHERE &#13;&#10;              INTERNO.ST='A' AND&#13;&#10;              COALESCE(INTERNO.EM_TRANSITO,'N')='N' AND                  &#13;&#10;              INTERNO.idcela = CELA.id_cela&#13;&#10;    ), 0) TOTAL_PISO&#13;&#10;FROM CELA&#13;&#10;   INNER JOIN SOLARIO ON (CELA.idsolario = SOLARIO.id_solario)&#13;&#10;   INNER JOIN GALERIA ON (solario.idgaleria = GALERIA.ID_GALERIA)&#13;&#10;   INNER JOIN PAVILHAO ON (galeria.idpavilhao = PAVILHAO.ID_PAVILHAO)&#13;&#10;WHERE pavilhao.id_pavilhao=304    " Database="Database" PropData="05704C65667403AC000470546F7003AC000A506172616D65746572730100"/>
