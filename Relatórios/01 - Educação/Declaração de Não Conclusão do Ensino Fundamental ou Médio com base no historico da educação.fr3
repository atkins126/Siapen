    <TfrxDBXQuery Name="SqlHISTORICO" UserName="SqlHISTORICO" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select  (SELECT p.NOMELIMPO FROM SEM_FUGA (i.nome_interno) p) as nome_interno, i.foto, i.rgi, i.mae, i.pai, p.pavilhao, g.galeria,&#13;&#10;c.cela, s.solario, n.nome_up, e.historico, e.data_historico, e.data_saida,&#13;&#10;e.deficiencia, e.data_matricula, e.periodo, e.turma,  &#13;&#10;(SELECT p.NOMELIMPO FROM SEM_FUGA (i.nome_interno) p) || ' - ' || i.&#34;RGI&#34; as marcador,&#13;&#10;substr(i.cpf, 1, 3) ||'.'||substr(i.cpf, 4, 6)||'.'||substr(i.cpf, 7, 9)||'-'||substr(i.cpf, 10, 11) as cpf,                                                                                                                                                                                                                                                   &#13;&#10;i.rg, i.orgao_emissor_rg as orgao_expedidor,&#13;&#10;coalesce(pr.nprocesso,'Colocar Processo') as processo, pr.vara as vara, t.serie_estudo                                     &#13;&#10;from interno i&#13;&#10;   INNER JOIN PAVILHAO p ON (I.idpavilhao = P.id_pavilhao)&#13;&#10;   INNER JOIN GALERIA g ON (I.idgaleria = G.id_galeria)&#13;&#10;   INNER JOIN CELA c ON (I.idcela = C.id_cela)&#13;&#10;   INNER JOIN SOLARIO s ON (I.idsolario = S.id_solario)&#13;&#10;   INNER JOIN unidade_penal n ON (I.id_up = n.id_up)&#13;&#10;   INNER JOIN historico_estudo e ON (I.id_interno = e.id_interno)     &#13;&#10;   LEFT  JOIN processo pr ON (pr.idinterno = i.id_interno)&#13;&#10;   left join serie_estudo t on t.id_serie_estudo = i.idserieestudo       &#13;&#10;        &#13;&#10;WHERE I.ID_UP = :ID_UP&#13;&#10;AND COALESCE (e.id_historico_estudo, '') &#60;&#62; ''&#13;&#10;AND e.data_historico between :data and :datafinal&#13;&#10;AND e.historico like '%' || :texto || '%'                                                                                              &#13;&#10;&#13;&#10;ORDER BY i.nome_interno, e.data_historico    