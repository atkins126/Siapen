    <TfrxDBXQuery Name="Sqlmonitoramento" UserName="Sqlmonitoramento" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select m.observacao, m.data_evento, m.hora_evento,&#13;&#10;F.nome_funcionario as funcionario, f2.nome_funcionario as funcionariomonior,&#13;&#10;p.posto_trabalho, e.descricao_evento as evento, s.descricao_eventos as subeventos,&#13;&#10;q.equipe&#13;&#10;from mov_eventos m&#13;&#10;left join eventos e on (e.id_eventos = m.id_eventos)&#13;&#10;left join sub_eventos s on (s.id_sub_eventos = m.id_sub_eventos)&#13;&#10;left join posto_trabalho p on (p.idposto_trabalho = m.idposto_trabalho)&#13;&#10;left join funcionario f on (f.id_funcionario = m.idfuncionario)&#13;&#10;left join funcionario f2 on (f2.id_funcionario = m.idfuncionario_monitor)&#13;&#10;left join equipe q on (q.idequipe = m.idequipe)&#13;&#10;where m.data_evento between :data and :data1&#13;&#10;and m.id_up = :id_up and m.status = 'F' and&#13;&#10;q.equipe like :equipe || '%'&#13;&#10;order by q.equipe,m.data_evento, m.hora_evento&#13;&#10;&#13;&#10;    " Database="Database" PropData=""/>
