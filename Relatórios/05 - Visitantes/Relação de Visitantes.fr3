    <TfrxDBXQuery Name="sqlvisitante" UserName="sqlvisitante" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select v.visitante, a.idagenda_atendimento,&#13;&#10;v.rg, i.nome_interno, aa.descricao, g.grau_parentesco,&#13;&#10;c.cela, a.observacao                               &#13;&#10;from agenda_atendimento a&#13;&#10;left join visitante v on (a.id_visitante = v.id_visitante)&#13;&#10;left join interno i on (a.id_interno = i.id_interno)&#13;&#10;left join visitante_interno vi on (vi.id_visitante = v.id_visitante)&#13;&#10;left join grau_parentesco g on (vi.id_grau_parentesco = g.id_grau_parentesco)&#13;&#10;left join agenda_assuntos aa on (a.idagenda_assuntos = aa.idagenda_assuntos)&#13;&#10;left join agenda_situacao s on (a.idagenda_situacao = s.idagenda_situacao)&#13;&#10;left join cela c on (i.idcela = c.id_cela)&#13;&#10;left join pavilhao p on (p.id_pavilhao = i.idpavilhao)    &#13;&#10;where a.data_agenda between :data and :data1&#13;&#10;and a.id_up = :id_up and s.descricao like 'ATIVO' AND&#13;&#10;((aa.descricao like 'VISITA SOCIAL') or (aa.descricao like 'VISITA INTIMA')) and         &#13;&#10;p.pavilhao like :pavilhao || '%' and iif(substr(cast(a.hora_agenda as varchar(30)),1,2)&#60;=12, 'Manhã', 'Tarde') like :periodo || '%'&#13;&#10;order by a.data_agenda, i.nome_interno&#13;&#10;&#13;&#10;                                    " Database="Database" PropData=""/>
