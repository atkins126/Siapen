 &#13;&#10;      &#13;&#10;       dbx_filtro.close;                          &#13;&#10;       dbx_filtro.sql.text:= dbx_filtro.sql.text + ' where interno.id_interno = ' + lbl_id.caption;&#13;&#10;       dbx_filtro.open;&#13;&#10;&#13;&#10;       memo_um.lines.clear;&#13;&#10;       memo_um.lines.text:=                                            &#13;&#10;'[dbx_filtro.&#34;nome_interno&#34;] - Prontuário: [dbx_filtro.&#34;rgi&#34;]. ' + #13 +    &#13;&#10;'Localização: [dbx_filtro.&#34;sigla&#34;] - [dbx_filtro.&#34;galeria&#34;] - [dbx_filtro.&#34;solario&#34;] - [dbx_filtro.&#34;cela&#34;].';           &#13;&#10;&#13;&#10;       MEMO_NOME_LISTA.LINES.TEXT:='Visitantes cadastrados: ';                 &#13;&#10;&#13;&#10;       MEMO_MUITOS.LINES.CLEAR;&#13;&#10;       MEMO_MUITOS.LINES.TEXT:=&#13;&#10;'[dbx_filtro.&#34;visitante&#34;] ([dbx_filtro.&#34;grau_parentesco&#34;]) - CPF: [dbx_filtro.&#34;cpf&#34;]. ' + #13 +    &#13;&#10;'Data de nascimento: [dbx_filtro.&#34;data_nascimento&#34;]. ' + #13 +    &#13;&#10;'Última visita: [dbx_filtro.&#34;data_ult_visita&#34;].' + #13 +    &#13;&#10;'Endereço: [dbx_filtro.&#34;endereco&#34;].' + #13 + //n.º [dbx_filtro.&#34;numero&#34;] [dbx_filtro.&#34;complemento&#34;] - Bairro: [dbx_filtro.&#34;bairro&#34;] - [dbx_filtro.&#34;cidade&#34;]/[dbx_filtro.&#34;UF&#34;] - CEP: [dbx_filtro.&#34;CEP&#34;].' + #13 +  &#13;&#10;'Cidade: [dbx_filtro.&#34;cidade&#34;].'+#13+                                             &#13;&#10;'Telefone: [dbx_filtro.&#34;fone&#34;]. '+#13+&#13;&#10;'[dbx_filtro.&#34;outras_visitas&#34;]'+#13+#13;&#13;&#10;         &#13;&#10;                                                                       &#13;&#10;    end&#13;&#10;    else //selecionou visita                                          &#13;&#10;    begin&#13;&#10;       memo_nome_cabecalho.lines.text:='Dados do visitante: ';                                                                                                                                                    &#13;&#10;      &#13;&#10;       dbx_filtro.close;                                                                &#13;&#10;       dbx_filtro.sql.text:= dbx_filtro.sql.text + ' where visitante.id_visitante = ' + lbl_id.caption;                                   &#13;&#10;       dbx_filtro.open;                                                                                                                &#13;&#10;           &#13;&#10;       memo_um.lines.clear;&#13;&#10;       memo_um.lines.text:=                                            &#13;&#10;'[dbx_filtro.&#34;visitante&#34;] ([dbx_filtro.&#34;grau_parentesco&#34;]) - CPF: [dbx_filtro.&#34;cpf&#34;]. ' + #13 +    &#13;&#10;'Data de nascimento: [dbx_filtro.&#34;data_nascimento&#34;]. ' + #13 +    &#13;&#10;'Última visita: [dbx_filtro.&#34;data_ult_visita&#34;].' + #13 +    &#13;&#10;'Endereço: [dbx_filtro.&#34;endereco&#34;].' + #13 + //n.º [dbx_filtro.&#34;numero&#34;] [dbx_filtro.&#34;complemento&#34;] - Bairro: [dbx_filtro.&#34;bairro&#34;] - [dbx_filtro.&#34;cidade&#34;]/[dbx_filtro.&#34;UF&#34;] - CEP: [dbx_filtro.&#34;CEP&#34;].' + #13 +  &#13;&#10;'Cidade: [dbx_filtro.&#34;cidade&#34;].'+#13+                                             &#13;&#10;'Telefone: [dbx_filtro.&#34;fone&#34;]. ';&#13;&#10;  &#13;&#10;       MEMO_NOME_LISTA.LINES.TEXT:='Internos visitados: ';  &#13;&#10;  &#13;&#10;       MEMO_MUITOS.LINES.CLEAR;&#13;&#10;       MEMO_MUITOS.LINES.TEXT:=&#13;&#10;'[dbx_filtro.&#34;nome_interno&#34;] - Prontuário: [dbx_filtro.&#34;rgi&#34;]. ' + #13 +    &#13;&#10;'Localização: [dbx_filtro.&#34;sigla&#34;] - [dbx_filtro.&#34;galeria&#34;] - [dbx_filtro.&#34;solario&#34;] - [dbx_filtro.&#34;cela&#34;].';                   &#13;&#10;         &#13;&#10;           &#13;&#10;    end;              &#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end." PropData="">
