    <TfrxDBXQuery Name="dbx_visitas" UserName="dbx_visitas" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select &#13;&#10;    visitante.visitante||' - CPF: '||visitante.cpf as nome                &#13;&#10;from visitante&#13;&#10;where &#13;&#10;   TRIM(COALESCE(VISITANTE.VISITANTE, '')) &#60;&#62; '' and id_up = :id_up and trim(upper(visitante.status)) &#60;&#62; 'I'&#13;&#10;order by visitante.visitante                                                                 " Database="database" PropData="05704C65667403FC010470546F7002240A506172616D657465727301010C3F000000204E616D653D2269645F7570222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B49445F5550262336323B220000"/>
    <TfrxDBXQuery Name="dbx_filtro" UserName="dbx_filtro" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select &#13;&#10;    visitante.visitante,&#13;&#10;    coalesce(grau_parentesco.grau_parentesco, 'não informado') as grau_parentesco,&#13;&#10;    coalesce(visitante.cpf, 'não informado') as cpf,&#13;&#10;    coalesce(extract(day from visitante.data_ult_visita)||'/'||extract(month from visitante.data_ult_visita)||'/'||extract(year from visitante.data_ult_visita), 'não informado') as data_ult_visita,  &#13;&#10;    iif(visitante.endereco &#60;&#62; '', visitante.endereco||', n.º '|| coalesce(visitante.numero, 'não informado')|| iif(visitante.complemeneto &#60;&#62; '', visitante.complemeneto, '') ||' - Bairro: '|| coalesce(visitante.bairro, 'não informado')||iif(visitante.cep &#60;&#62; '', ' CEP: '||visitante.cep ,''), 'não informado')  as endereco,  &#13;&#10;    iif(cidade.cidade &#60;&#62; '',cidade.cidade||'/'||cidade.uf, 'não informada') as cidade,                                                                                                                                                  &#13;&#10;  --  coalesce(, 'não informado') as cidade,&#13;&#10; --   coalesce(, 'não informado') as uf,  &#13;&#10; --   coalesce(visitante.cep, 'não informado') as cep,  &#13;&#10;    coalesce(visitante.fone, 'não informado') as fone,  &#13;&#10;    coalesce(extract(day from visitante.data_nascimento)||'/'||extract(month from visitante.data_nascimento)||'/'||extract(year from visitante.data_nascimento), 'não informado') as data_nascimento,  &#13;&#10;    interno.nome_interno,&#13;&#10;    interno.rgi,&#13;&#10;    galeria.galeria,&#13;&#10;    solario.solario,&#13;&#10;    cela.cela,&#13;&#10;    unidade_penal.nome_up,&#13;&#10;    unidade_penal.sigla,&#13;&#10;&#13;&#10;coalesce(&#13;&#10;'Também visita: ' || (&#13;&#10;select&#13;&#10;    list(i.nome_interno, ', ' ) outros_internos&#13;&#10;from interno i&#13;&#10;   inner join visitante_interno vi on (i.id_interno = vi.id_interno)&#13;&#10;where &#13;&#10;      (vi.id_interno &#60;&#62; interno.id_interno) and (vi.id_visitante = visitante.id_visitante)                                                                                  &#13;&#10;) || '.', 'Não visita outros internos.')                                                                         &#13;&#10;&#13;&#10;&#13;&#10; as outras_visitas&#13;&#10;&#13;&#10;from interno&#13;&#10;   inner join visitante_interno on (interno.id_interno = visitante_interno.id_interno)&#13;&#10;   inner join visitante on (visitante_interno.id_visitante = visitante.id_visitante)&#13;&#10;   inner join cidade on (visitante.idcidade = cidade.id_cidade)&#13;&#10;   inner join grau_parentesco on (visitante_interno.id_grau_parentesco = grau_parentesco.id_grau_parentesco)&#13;&#10;   inner join cela on (interno.idcela = cela.id_cela)&#13;&#10;   inner join solario on (interno.idsolario = solario.id_solario)&#13;&#10;   inner join galeria on (interno.idgaleria = galeria.id_galeria)&#13;&#10;   inner join unidade_penal on (interno.id_up = unidade_penal.id_up)&#13;&#10;&#13;&#10;                " Database="database" PropData="05704C65667403D8010470546F7003A0000A506172616D65746572730100"/>
