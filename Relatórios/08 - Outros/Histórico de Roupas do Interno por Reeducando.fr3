    <TfrxDBXQuery Name="Sqlup" UserName="Sqlup" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT u.nome_up, u.endereco, u.bairro, u.numero, u.complemento, u.cep,&#13;&#10;c.cidade||' - '||c.uf as cidade, u.fone, u.fax, u.email                     &#13;&#10;from unidade_penal u&#13;&#10;inner join cidade c on(c.id_cidade=u.id_cidade)                                                       &#13;&#10;WHERE ID_UP = :ID_UP  " Database="Database" PropData="05704C65667403F0000470546F70021C0A506172616D657465727301010C3F000000204E616D653D2249445F5550222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B49445F5550262336323B220000"/>
    <TfrxDBXQuery Name="SqlRoupas" UserName="SqlRoupas" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select&#13;&#10;r.id_interno, i.nome_interno, g.galeria as bloco, s.solario as ala, c.cela, v.vestimentas, ri.qtde, r.data, i.pai, i.mae, i.vulgo, i.rgi&#13;&#10;&#13;&#10;from roupas r&#13;&#10;inner join interno i on (r.id_interno = i.id_interno)&#13;&#10;inner join unidade_penal up on (i.id_up = up.id_up)&#13;&#10;inner join solario s on (i.idsolario = s.id_solario)&#13;&#10;inner join galeria g on (i.idgaleria = g.id_galeria)&#13;&#10;inner join cela c on (i.idcela = c.id_cela)&#13;&#10;inner join roupas_interno ri on (r.id_roupas = ri.id_roupa)&#13;&#10;inner join vestimentas v on (v.id_vestimentas = ri.id_vestimentas )&#13;&#10;&#13;&#10;WHERE i.ST = 'A' &#13;&#10;and i.ID_UP = :ID_UP &#13;&#10;and i.nome_interno like '%'||(:INTERNO)||'%'&#13;&#10;&#13;&#10;order by bloco, ala, cela, i.nome_interno, r.data&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;                                                   " Database="Database" PropData=""/>
