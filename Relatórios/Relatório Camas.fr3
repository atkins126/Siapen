    <TfrxDBXQuery Name="SqlDados" UserName="SqlDados" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select  DISTINCT i.nome_interno, c.cela, s.solario, p.pavilhao, i.rgi, G.GALERIA, i.data_entrada, FD.DT_INICIO_ISOLAMENTO,&#13;&#10;IIF(S.SOLARIO = 'ALA D - CPP', 'CASTIGO', IIF(S.SOLARIO = 'ALA E - CPP', 'ESPECIAL', '')) AS OBS,&#13;&#10;IIF(COALESCE(I.nome_interno, '') = '', 'VAZIA', 'OCUPADA') AS STATUS&#13;&#10;    &#13;&#10;from cela c&#13;&#10;       inner join solario s on (c.idsolario = s.id_solario)&#13;&#10;       inner join pavilhao p on (c.idpavilhao = p.id_pavilhao)&#13;&#10;       inner join GALERIA G on (c.idGALERIA = G.id_GALERIA)&#13;&#10;       inner join unidade_penal up on (p.id_up = up.id_up)&#13;&#10;       left join interno i on (i.idcela = c.id_cela)&#13;&#10;       left join VINC_FALTA_DISCIPLINAR FD on (I.ID_INTERNO = FD.id_vinc_falta_disciplinar)&#13;&#10;where p.id_up = :id_up&#13;&#10;AND COALESCE(S.solario, '') &#60;&#62; 'VIRTUAL'    &#13;&#10;order by s.solario, c.cela                 &#13;&#10;  " Database="Database" PropData="05704C6566740390000470546F7002680A506172616D657465727301010C3F000000204E616D653D2269645F7570222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B49445F5550262336323B220000"/>
