    <TfrxDBXQuery Name="SQL_TRABALHO" UserName="SQL_TRABALHO" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select &#13;&#10;    'Classificado em ' || iif(historico_trabalho.data &#60;&#62; null,  extract(day from historico_trabalho.data)||'/'||EXTRACT(month from historico_trabalho.data)||'/'||extract(year from historico_trabalho.data), 'data não informada')|| iif(historico_trabalho.documento &#60;&#62; '', ' (OS n.º ' || historico_trabalho.documento || ')', '(número da OS não informado)')  || '.' as classificacao,&#13;&#10;    'Setor: ' || coalesce(setor_trabalho.setor_trabalho, 'Não informado') || ' - Local: ' || coalesce(local_trabalho.local_trabalho, 'Não informado')||'.' as setor,&#13;&#10;    iif(historico_trabalho.motivo_saida &#60;&#62;'', 'Desclassificado em ' || COALESCE(extract(day from historico_trabalho.data_saida)||'/'||extract(month from historico_trabalho.data_saida)||'/'||extract(year from historico_trabalho.data_saida), '&#60;data de desclassificação não informada&#62;') || '. '||iif(historico_trabalho.motivo_saida &#60;&#62; '', ' Motivo: ' || historico_trabalho.motivo_saida ||'.', ' Motivo de desclassificação não informado.') || iif(historico_trabalho.documento_desclassificacao &#60;&#62; '', ' OS de desclassificação n.º ' || historico_trabalho.documento_desclassificacao, ' Número da OS de desclassificação não informada')||'.', 'Status: não desclassificado.') as desclassificacao&#13;&#10;from historico_trabalho&#13;&#10;   inner join interno on (historico_trabalho.id_interno = interno.id_interno)&#13;&#10;   inner join setor_trabalho on (historico_trabalho.id_setor_trabalho = setor_trabalho.id_setor_trabalho)&#13;&#10;   inner join funcao_interno on (historico_trabalho.id_funcao_interno = funcao_interno.id_funcao_interno)&#13;&#10;   inner join local_trabalho on (historico_trabalho.id_local_trabalho = local_trabalho.id_local_trabalho)&#13;&#10;where&#13;&#10;  " Database="database" PropData="05704C6566740308010470546F700384010A506172616D6