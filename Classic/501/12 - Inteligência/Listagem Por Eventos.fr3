    <TfrxDBXQuery Name="Sqlmovevento" UserName="Sqlmovevento" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select m.observacao, m.data_evento, m.hora_evento,&#13;&#10;F.nome_funcionario as funcionario, f2.nome_funcionario as funcionariomonior,&#13;&#10;p.posto_trabalho, e.descricao_evento as evento, s.descricao_eventos as subeventos,&#13;&#10;q.equipe, u.nome_up                                    &#13;&#10;from mov_eventos m&#13;&#10;left join eventos e on (e.id_eventos = m.id_eventos)&#13;&#10;left join sub_eventos s on (s.id_sub_eventos = m.id_sub_eventos)&#13;&#10;left join posto_trabalho p on (p.idposto_trabalho = m.idposto_trabalho)&#13;&#10;left join funcionario f on (f.id_funcionario = m.idfuncionario)&#13;&#10;left join funcionario f2 on (f2.id_funcionario = m.idfuncionario_monitor)&#13;&#10;left join equipe q on (q.idequipe = m.idequipe)&#13;&#10;left join unidade_penal u on (u.id_up = m.id_up)    &#13;&#10;where m.data_evento between :data and :data1&#13;&#10;and m.id_up = :id_up and m.status = 'F' and descricao_evento like :evento || '%'                                                                                                                              &#13;&#10;order by  e.descricao_evento,m.data_evento, m.hora_evento&#13;&#10;&#13;&#10;    " Database="Database" PropData=""/>
    <TfrxDBXQuery Name="Sqlup" UserName="Sqlup" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT u.nome_up, u.endereco, u.bairro, u.numero, u.complemento, u.cep,&#13;&#10;c.cidade||' - '||c.uf as cidade, u.fone, u.fax, u.email                     &#13;&#10;from unidade_penal u&#13;&#10;inner join cidade c on(c.id_cidade=u.id_cidade)                                                       &#13;&#10;WHERE ID_UP = :ID_UP&#13;&#10;  " Database="Database" PropData="05704C65667402600470546F7002240A506172616D657465727301010C3F000000204E616D653D2249445F5550222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B49445F5550262336323B220000"/>
