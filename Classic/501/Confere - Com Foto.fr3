    <TfrxDBXQuery Name="Sqlconfere" UserName="Sqlconfere" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT &#13;&#10;    CELA.CELA,&#13;&#10;    INTERNO.NOME_INTERNO,&#13;&#10;    INTERNO.FOTO,                                &#13;&#10;    NULL DATA_ENTRADA,&#13;&#10;    PAVILHAO.PAVILHAO,&#13;&#10;    GALERIA.GALERIA,&#13;&#10;    CELA.CELA,&#13;&#10;    SOLARIO.SOLARIO,&#13;&#10;    COALESCE((SELECT COUNT(*)              &#13;&#10;              FROM INTERNO                         &#13;&#10;              WHERE &#13;&#10;              INTERNO.ST='A' AND&#13;&#10;              COALESCE(INTERNO.EM_TRANSITO,'N')='N' AND                  &#13;&#10;              INTERNO.idcela = CELA.id_cela&#13;&#10;    ), 0) TOTAL_PISO&#13;&#10;FROM CELA&#13;&#10;   INNER JOIN SOLARIO ON (CELA.idsolario = SOLARIO.id_solario)&#13;&#10;   INNER JOIN GALERIA ON (solario.idgaleria = GALERIA.ID_GALERIA)&#13;&#10;   INNER JOIN PAVILHAO ON (galeria.idpavilhao = PAVILHAO.ID_PAVILHAO)&#13;&#10;   LEFT JOIN (&#13;&#10;   SELECT INTERNO.NOME_INTERNO, INTERNO.IDCELA, INTERNO.FOTO&#13;&#10;    FROM INTERNO                         &#13;&#10;    WHERE &#13;&#10;    INTERNO.ST='A' AND&#13;&#10;    COALESCE(INTERNO.EM_TRANSITO,'N')='N'          &#13;&#10;     ) AS INTERNO ON (INTERNO.IDCELA= CELA.ID_CELA)              &#13;&#10;WHERE pavilhao.PAVILHAO like :PAVILHAO || '%' AND PAVILHAO.ID_UP=:ID_UP&#13;&#10;ORDER BY pavilhao.PAVILHAO, CELA.CELA                                                                       " Database="Database" PropData=""/>
    <TfrxDBXQuery Name="Sqlup" UserName="Sqlup" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT u.nome_up, u.endereco, u.bairro, u.numero, u.complemento, u.cep,&#13;&#10;c.cidade||' - '||c.uf as cidade, u.fone, u.fax, u.email                     &#13;&#10;from unidade_penal u&#13;&#10;inner join cidade c on(c.id_cidade=u.id_cidade)                                                       &#13;&#10;WHERE ID_UP = :ID_UP&#13;&#10;  " Database="Database" PropData="05704C65667402780470546F70025C0A506172616D657465727301010C3F000000204E616D653D2249445F5550222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B49445F5550262336323B220000"/>
