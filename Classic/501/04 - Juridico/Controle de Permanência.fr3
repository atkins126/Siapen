    <TfrxDBXQuery Name="Sqlcontrole" UserName="Sqlcontrole" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select&#13;&#10; i.nome_interno,&#13;&#10; i.data_entrada,&#13;&#10;-- i.rgi,&#13;&#10; p.proedencia,&#13;&#10;   (select first 1&#13;&#10;    cast (extract (day from cp.data_final)||'.'||extract (month from cp.data_final)||'.'||extract(year from cp.data_final) as date) - current_date  as diasrestantes&#13;&#10;    from controle_permanencia cp&#13;&#10;    where i.id_interno = cp.id_interno and coalesce(cp.data_final,'') &#60;&#62; ''&#13;&#10;    order by cp.id_controle_permanencia desc),&#13;&#10;&#13;&#10;    (select first 1&#13;&#10;    cp.diasautorizados&#13;&#10;    from controle_permanencia cp&#13;&#10;    where i.id_interno = cp.id_interno and coalesce(cp.data_final,'') &#60;&#62; ''&#13;&#10;    order by cp.id_controle_permanencia desc) as prazo,&#13;&#10;&#13;&#10;    (select first 1&#13;&#10;    cp.data_final&#13;&#10;    from controle_permanencia cp&#13;&#10;    where i.id_interno = cp.id_interno and coalesce(cp.data_final,'') &#60;&#62; ''&#13;&#10;    order by cp.id_controle_permanencia desc) as retorno,&#13;&#10;&#13;&#10;    (select first 1&#13;&#10;    cp.obs&#13;&#10;    from controle_permanencia cp&#13;&#10;    where i.id_interno = cp.id_interno and coalesce(cp.data_final,'') &#60;&#62; ''&#13;&#10;    order by cp.id_controle_permanencia desc) as observacao                                  &#13;&#10;from interno i&#13;&#10;inner join procedencia p on i.id_procedencia = p.id_procedencia&#13;&#10;where i.id_up = :id_up and i.st = 'A'                                               &#13;&#10;order by i.nome_interno&#13;&#10;      " Database="Database" PropData="05704C65667403AC000470546F70025C0A506172616D657465727301010C3F000000204E616D653D2269645F7570222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B49445F5550262336323B220000"/>
    <TfrxDBXQuery Name="Sqlup" UserName="Sqlup" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT u.nome_up, u.endereco, u.bairro, u.numero, u.complemento, u.cep,&#13;&#10;c.cidade||' - '||c.uf as cidade, u.fone, u.fax, u.email                     &#13;&#10;from unidade_penal u&#13;&#10;inner join cidade c on(c.id_cidade=u.id_cidade)                                                       &#13;&#10;WHERE ID_UP = :ID_UP&#13;&#10;  " Database="Database" PropData="05704C65667402600470546F7002240A506172616D657465727301010C3F000000204E616D653D2249445F5550222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B49445F5550262336323B220000"/>
