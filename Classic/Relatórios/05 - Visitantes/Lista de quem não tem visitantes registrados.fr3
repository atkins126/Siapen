    <TfrxDBXQuery Name="sqlvisitante" UserName="sqlvisitante" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select i.nome_interno, p.pavilhao, g.galeria, s.solario, c.cela                                                                                                                        &#13;&#10;from interno i&#13;&#10;left join visitante_interno vi on (vi.ID_INTERNO = i.ID_INTERNO)    &#13;&#10;inner join pavilhao p on (i.idpavilhao = p.id_pavilhao)                &#13;&#10;inner join galeria g on (i.idgaleria = g.id_galeria)&#13;&#10;inner join solario s on (i.idsolario = s.id_solario)&#13;&#10;inner join cela c on (i.idcela = c.id_cela)    &#13;&#10;  &#13;&#10;where vi.id_interno is null and i.id_up = :id_up and i.ST = 'A'                                              &#13;&#10;and i.nome_interno like '%'||(:nome_interno)||'%'&#13;&#10;order by p.pavilhao, g.galeria, s.solario, c.cela, i.nome_interno                                                  " Database="Database" PropData=""/>
