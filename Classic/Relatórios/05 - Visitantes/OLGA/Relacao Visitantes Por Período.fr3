    <TfrxDBXQuery Name="sqlvisitantesDia" UserName="sqlvisitantesDia" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select&#13;&#10;        iif(mdv.sexo='F','Feminino','Masculino') sexo,&#13;&#10;        p.pavilhao,mdv.quantidade_menor,    &#13;&#10;        count(*)&#13;&#10;from mov_diario_visitantes mdv&#13;&#10;join visitante_interno vi on (mdv.id_visitante=vi.id_visitante)&#13;&#10;join interno i on (vi.id_interno=i.id_interno)&#13;&#10;join pavilhao p on (i.idpavilhao=p.id_pavilhao)&#13;&#10;where mdv.data_visita between :DATA and :DATA2      &#13;&#10;group by iif(mdv.sexo='F','Feminino','Masculino'),&#13;&#10;p.pavilhao,mdv.quantidade_menor&#13;&#10;&#13;&#10;     " Database="database" PropData=""/>
