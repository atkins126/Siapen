    <TfrxDBXQuery Name="Sqltrabalho" UserName="Sqltrabalho" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT                    &#13;&#10;    INTERNO.NOME_INTERNO,&#13;&#10;    SOLARIO.SOLARIO,&#13;&#10;    LOCAL_TRABALHO.local_trabalho,&#13;&#10;    interno.horario_saida_saidao,&#13;&#10;    interno.horario_entrada_saidao,  &#13;&#10;    interno.corte,&#13;&#10;    INTERNO.TURMA_SAIDAO,&#13;&#10;    interno.horario_saida_sabado,&#13;&#10;    interno.horario_entrada_sabado,&#13;&#10;    interno.horario_saida_domingo,&#13;&#10;    interno.horario_entrada_domingo&#13;&#10;&#13;&#10;FROM INTERNO&#13;&#10;   inner join  unidade_penal ON (interno.id_up=unidade_penal.id_up)&#13;&#10;   INNER JOIN CELA ON (INTERNO.IDCELA = CELA.ID_CELA)&#13;&#10;   INNER JOIN SOLARIO ON (INTERNO.IDSOLARIO = SOLARIO.ID_SOLARIO)&#13;&#10;   INNER JOIN local_trabalho ON (INTERNO.id_local_trabalho = local_trabalho.id_local_trabalho)&#13;&#10;WHERE INTERNO.ST = 'A'&#13;&#10;AND INTERNO.ID_UP = :ID_UP &#13;&#10;AND SOLARIO.SOLARIO LIKE '%' || :ALA || '%'&#13;&#10;and coalesce(INTERNO.TURMA_SAIDAO, '') = '1'    &#13;&#10;ORDER BY solario.solario&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;                    &#13;&#10;&#13;&#10;                                  " Database="Database" PropData=""/>
    <TfrxDBXQuery Name="Sqlup" UserName="Sqlup" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT u.nome_up, u.endereco, u.bairro, u.numero, u.complemento, u.cep,&#13;&#10;c.cidade||' - '||c.uf as cidade, u.fone, u.fax, u.email                     &#13;&#10;from unidade_penal u&#13;&#10;inner join cidade c on(c.id_cidade=u.id_cidade)                                                       &#13;&#10;WHERE ID_UP = :ID_UP&#13;&#10;  " Database="Database" PropData="05704C656674025C0470546F70025C0A506172616D657465727301010C3F000000204E616D653D2249445F5550222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B49445F5550262336323B220000"/>
    <TfrxDBXQuery Name="SqlAla" UserName="SqlAla" CloseDataSource="True" FieldAliases.Text="NOME_INTERNO=NOME_INTERNO&#13;&#10;ID_INTERNO=ID_INTERNO" SQL.Text="select distinct S.SOLARIO&#13;&#10;from interno i&#13;&#10;       inner join SOLARIO S on (S.id_solario = i.idsolario)&#13;&#10;where i.id_up = :id_up                                             &#13;&#10;and coalesce(i.nome_interno, '')&#60;&#62;''&#13;&#10;order by S.SOLARIO  " Database="Database" PropData="05704C656674025C0470546F7002200A506172616D657465727301010C3F000000204E616D653D2269645F7570222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B49445F5550262336323B220000"/>
