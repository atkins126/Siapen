    <TfrxDBXQuery Name="Sqlup" UserName="Sqlup" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT u.nome_up, u.endereco, u.bairro, u.numero, u.complemento, u.cep,&#13;&#10;c.cidade||' - '||c.uf as cidade, u.fone, u.fax, u.email,u.sigla                                     &#13;&#10;from unidade_penal u&#13;&#10;inner join cidade c on(c.id_cidade=u.id_cidade)                                                       &#13;&#10;WHERE ID_UP = :ID_UP  " Database="Database" PropData="05704C65667403D4000470546F70021C0A506172616D657465727301010C3F000000204E616D653D2249445F5550222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B49445F5550262336323B220000"/>
    <TfrxDBXQuery Name="sqlListaInterno" UserName="sqlListaInterno" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select&#13;&#10;i.nome_interno,&#13;&#10;i.rgi,i.id_interno,&#13;&#10;hi.data_hora,hi.motivo_saida,&#13;&#10;hi.descricao, hi.tipo_historico,&#13;&#10;p.pavilhao, g.galeria, s.solario, c.cela  &#13;&#10;  &#13;&#10;from interno i&#13;&#10;                                                   &#13;&#10;INNER JOIN historico_interno hi ON (hi.idinterno = i.id_interno)&#13;&#10;INNER JOIN unidade_penal up ON (up.id_up = i.id_up)&#13;&#10;INNER JOIN pavilhao p ON (i.idpavilhao = p.id_pavilhao)    &#13;&#10;INNER JOIN GALERIA G ON (i.idgaleria = g.id_galeria)&#13;&#10;INNER JOIN SOLARIO S ON (i.idsolario = s.id_solario)&#13;&#10;INNER JOIN CELA C ON (i.idcela = c.id_cela)&#13;&#10;  &#13;&#10;WHERE i.ID_UP = :ID_UP&#13;&#10;AND ( (i.NOME_INTERNO||' ('||i.RGI||')'||' ID:'||i.id_interno = :nome_interno ) )&#13;&#10;AND (hi.status = 'S' or hi.status = 'E')&#13;&#10;AND hi.motivo_saida &#60;&#62; 'Mudança de Cela'                                                                      &#13;&#10;  &#13;&#10;ORDER BY hi.data_hora desc, hi.idHistorico_Interno desc                                                                                      " Database="Database" PropData=""/>
