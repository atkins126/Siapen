    <TfrxDBXQuery Name="sqlTransferencia" UserName="sqlTransferencia" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT&#13;&#10; i.rgi, i.nome_interno, i.pai, i.mae, i.vulgo,                                                                               &#13;&#10; t.Documento, ha.data_inicial, ha.data_final, ha.qtde_horas, i.turma, e.serie_estudo                                                                                                                                                                                                                       &#13;&#10;from interno i&#13;&#10;inner join horas_aula ha on (i.id_interno = ha.id_interno)  &#13;&#10;INNER JOIN historico_trabalho t ON (I.id_interno = t.id_interno)&#13;&#10;INNER JOIN funcao_interno f ON (t.id_funcao_interno = f.id_funcao_interno)&#13;&#10;INNER JOIN serie_estudo e ON (I.idserieestudo = e.id_serie_estudo)    &#13;&#10;WHERE f.funcao_interno = 'ALUNO' and ha.data_inicial &#62;= :data1 and ha.data_final &#60;=:data2&#13;&#10;AND I.TURMA LIKE '%' || :TURMA&#13;&#10;order by e.serie_estudo, i.nome_interno                                                  &#13;&#10;  &#13;&#10;&#13;&#10;&#13;&#10;                                                   " Database="Database" PropData=""/>
