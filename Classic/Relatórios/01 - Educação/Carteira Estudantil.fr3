    <TfrxDBXQuery Name="SqlEstudo" UserName="SqlEstudo" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="select i.nome_interno, i.foto, p.pavilhao, g.galeria,&#13;&#10;c.cela, s.solario, e.serie_estudo, n.nome_up, i.RGI                                                                                                                    &#13;&#10;&#13;&#10;&#13;&#10;from interno i&#13;&#10;&#13;&#10;&#13;&#10;   INNER JOIN PAVILHAO p ON (I.idpavilhao = P.id_pavilhao)&#13;&#10;   INNER JOIN GALERIA g ON (I.idgaleria = G.id_galeria)&#13;&#10;   INNER JOIN CELA c ON (I.idcela = C.id_cela)&#13;&#10;   INNER JOIN SOLARIO s ON (I.idsolario = S.id_solario)&#13;&#10;   INNER JOIN serie_estudo e ON (I.idserieestudo = e.id_serie_estudo)&#13;&#10;   INNER JOIN unidade_penal n ON (I.id_up = n.id_up)&#13;&#10;&#13;&#10;WHERE I.ST = 'A' AND I.ID_UP = :ID_UP AND I.NOME_INTERNO LIKE '%' || :NOMEINTERNO || '%' &#13;&#10;AND COALESCE (I.IDSERIEESTUDO, '') &#60;&#62; '' and i.data_matricula between :data and :datafinal  &#13;&#10;  &#13;&#10;&#13;&#10;ORDER BY I.NOME_INTERNO      " Database="database" PropData=""/>
