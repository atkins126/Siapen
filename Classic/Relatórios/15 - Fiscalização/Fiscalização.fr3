    <TfrxDBXQuery Name="sqlFiscalizacao" UserName="sqlFiscalizacao" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT &#13;&#10;    INTERNO.NOME_INTERNO,&#13;&#10;    interno.vulgo,&#13;&#10;    INTERNO.RGI,&#13;&#10;    interno.mae,&#13;&#10;    interno.pai,&#13;&#10;    interno.endereco,&#13;&#10;    interno.cor_cabelos,&#13;&#10;    interno.cor_olhos,&#13;&#10;    interno.sinais_particulares,&#13;&#10;    interno.altura,&#13;&#10;    interno.peso,&#13;&#10;    interno.outro_nome,          &#13;&#10;    interno.foto,      &#13;&#10;    interno.endereco,                                                    &#13;&#10;    interno.tiporegime,&#13;&#10;    interno.condenacao, &#13;&#10;    interno.sexo,&#13;&#10;    interno.data_nascimento,        &#13;&#10;    profissao,&#13;&#10;    unidade_penal.sigla,&#13;&#10;    PAVILHAO.PAVILHAO,&#13;&#10;    GALERIA.GALERIA,&#13;&#10;    CELA.CELA,&#13;&#10;    SOLARIO.SOLARIO,&#13;&#10;    cidade.cidade        &#13;&#10;FROM INTERNO&#13;&#10;   Left join profissao on (profissao.id_profissao=interno.idprofissao)&#13;&#10;   LEFT JOIN CIDADE ON(interno.idcidade=cidade.id_cidade)&#13;&#10;   JOIN vinc_saidao ON (interno.id_interno=vinc_saidao.id_interno)&#13;&#10;   join saidao on (saidao.id_saidao=vinc_saidao.id_saidao)&#13;&#10;   LEFT join  unidade_penal ON (interno.id_up=unidade_penal.id_up)&#13;&#10;   LEFT JOIN PAVILHAO ON (INTERNO.IDPAVILHAO = PAVILHAO.ID_PAVILHAO)&#13;&#10;   INNER JOIN GALERIA ON (INTERNO.IDGALERIA = GALERIA.ID_GALERIA)&#13;&#10;   INNER JOIN CELA ON (INTERNO.IDCELA = CELA.ID_CELA)&#13;&#10;   INNER JOIN SOLARIO ON (INTERNO.IDSOLARIO = SOLARIO.ID_SOLARIO)                 &#13;&#10;WHERE INTERNO.ST = 'A'  &#13;&#10;ORDER BY  INTERNO.NOME_INTERNO            " Database="database" PropData="05704C65667403B4000470546F7002640A506172616D65746572730100"/>
    <TfrxDBXQuery Name="Sqlup" UserName="Sqlup" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT u.nome_up, u.endereco, u.bairro, u.numero, u.complemento, u.cep,&#13;&#10;c.cidade||' - '||c.uf as cidade, u.fone, u.fax, u.email                     &#13;&#10;from unidade_penal u&#13;&#10;left join cidade c on(c.id_cidade=u.id_cidade)                                                       &#13;&#10;WHERE ID_UP = :ID_UP  " Database="database" PropData="05704C6566740314010470546F7002240A506172616D657465727301010C3F000000204E616D653D2249445F5550222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B49445F5550262336323B220000"/>
